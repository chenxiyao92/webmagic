<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cxyhome.webmagic.mapper.TrademarkMapper">


        <select id="getTrademarkById" resultType="Trademark">
          select  * from tb_trademark where id = ${id};
         </select>


        <insert id="insertTmClassificationInfo" parameterType="Trademark">
          insert  into tm_info(id,mid) values (#{trademark.id},#{trademark.id});
        </insert>

</mapper>